(define (average x y) (/ (+ x y) 2))
(define (square x) (* x x))
(define (sqrt x)
  (define (improve G) (average G (/ x G)))
  (define (goodenough G) (< (abs (- (square G) x)) .00001))
  (define (try G)
    (if (goodenough G)
        G
        (try (improve G))))
  (try 1.0))
(sqrt 80)
(sqrt 81)
(sqrt 99)
(define (f*c a b) (* a b))
(define (f* a b) (- a b))
(define (fff a b) (* a b))